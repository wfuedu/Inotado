<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="edu.wfu.inotado.model.ScRubricDTO" table="WFU_SC_RUBRIC">
		<id name="id" type="integer" column="ID"/>
		<property name="name" type="string" length="255" column="NAME" />
		<property name="annotation" type="string" length="255"
			column="ANNOTATION" />
		<property name="baseScore" type="double" column="BASE_SCORE" />
		<property name="prefectScore" type="double" column="PREFECT_SCORE" />
		<property name="createdAt" type="timestamp" column="CREATED_AT" />
		<property name="updatedAt" type="timestamp" column="UPDATED_AT" />
	</class>

</hibernate-mapping>

