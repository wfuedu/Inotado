<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="edu.wfu.inotado.model.ScGradeDTO" table="WFU_SC_GRADE">
		<id name="id" type="integer" column="ID" />
		<property name="gradeId" type="integer" column="GB_GRADE_RECORD_T_ID"
			not-null="true" index="IDX_GRADE_ID" />
		<property name="rubricId" type="integer" column="RUBRIC_ID"
			not-null="true" index="IDX_RUBRIC_ID" />
		<property name="createdAt" type="timestamp" column="CREATED_AT" />
		<property name="updatedAt" type="timestamp" column="UPDATED_AT" />
	</class>

</hibernate-mapping>

